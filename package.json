{
  "name": "pequi-farm",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/pequifinance/pequi-farm.git",
  "author": "Gustavo <gustavo@obex.pw>",
  "license": "MIT",
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "postinstall": "truffle compile && yarn generate-types",
    "migrate": "tsc -p ./tsconfig.migrate.json --out-dir ./migrations && truffle migrate",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@openzeppelin/truffle-upgrades": "^1.7.0",
    "@truffle/hdwallet-provider": "^1.4.0",
    "@typechain/truffle-v5": "^2.0.0",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^15.3.0",
    "@types/web3": "^1.2.2",
    "truffle": "^5.1.22",
    "typechain": "2.0.0",
    "typescript": "4.1.5"
  }
}
